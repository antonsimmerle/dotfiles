syntax on
filetype on
colorscheme desert
set noswapfile
set cursorcolumn
set number
set expandtab
set tabstop=8
set softtabstop=4
set shiftwidth=4
set autoindent
set nobackup
set hlsearch
set showmatch
inoremap " ""<left>
inoremap ' ''<left>
inoremap ( ()<left>
inoremap [ []<left>
inoremap { {}<left>
inoremap {<CR> {<CR>}<ESC>O
inoremap {;<CR> {<CR>};<ESC>O